:test	a			:LimesSpecs ;
			:hasSource		:test ;
			:hasTarget		:test2 ;
			:hasMetric		:metric ;
			:hasReview		:review ;
			:hasAcceptance :acceptance .
		
:test	        a			:SourceDataset ;
			rdfs:label		"test";
			:endPoint		"/home/kleanthi/Documents/GitHub/HOBBIT/workpackage3/T3.1/src/test/resources/data/platform_data/dataGenerator/dereferenced_data.ttl";
			:type			"ttl";
			:variable		"?x" ;
			:pageSize		"-1";
			:restriction 		"?x a myOntology:isEvent" ;
			:property 		"myOntology:hasTimePoint/myOntology:beginTime RENAME beginDate" .
			

:test2	        a			:TargetDataset ;
			rdfs:label		"test";
			:endPoint		"/home/kleanthi/Documents/GitHub/HOBBIT/workpackage3/T3.1/src/test/resources/data/platform_data/dataGenerator/dereferenced_data.ttl";
			:type			"ttl";
			:variable		"?x" ;
			:pageSize		"-1";
			:restriction 		"?x a myOntology:isEvent" ;
			:property 		"myOntology:hasTimePoint/myOntology:beginTime RENAME beginDate" .
 					

:metric		a				:Metric ;
			:expression		"tmp_equal(x.beginDate,y.beginDate)" .

:review		a				:Review ;
			:threshold 		"0" ;
			:file 			"resources/equalEventsReview.ttl";
			:relation 		"" .

:acceptance	a			:Acceptance ;
			:threshold 		"1" ;
			:file 			"resources/equalEvents.ttl";
			:relation 		"myOntology:tmp_equal".


